<view wx:if="{{!loading && iShow}}" bindtap='clickButton'  class="geetest_holder geetest_wind {{css}}" style='width:{{styleConfig.btnWidth}}'>
  <view class="geetest_btn">
    <view class="geetest_radar_btn">
      <view class="geetest_radar">
        <view class="geetest_ring" style="box-shadow: inset 0 0 0 1px {{styleConfig.color}};background-color:{{styleConfig.color}}60;">
          <view class="geetest_small"></view>
        </view>
        <view class="geetest_sector">
          <view class="geetest_small"></view>
        </view>
        <view class="geetest_cross">
          <view class="geetest_h"></view>
          <view class="geetest_v"></view>
        </view>
        <view class="geetest_dot"></view>
        <view class="geetest_scan" style='background-color:{{styleConfig.color}}'>
          <view class="geetest_h"></view>
        </view>
        <view class="geetest_status">
          <view class="geetest_bg"></view>
        <view class="geetest_hook"></view>
        </view>
      </view>
      <view class="geetest_radar_tip">
        <span class="geetest_radar_tip_content">{{lang_tips.fullpage.btnTips}}</span>
        <span class="geetest_reset_tip_content" catchtap="refreshToReset">{{lang_tips.fullpage.reset}}</span>
        <span class="geetest_radar_error_code">{{errorTips}}</span>
      </view>
      <label class="geetest_logo" target="_blank" style="display:{{logodp}}"></label>
      <view class="geetest_other_offline  geetest_offline"></view>
    </view>
    <view class="geetest_ghost_success {{success_animate}}">
      <view class="geetest_success_btn">
        <view class="geetest_success_box">
          <view class="geetest_success_show">
            <view class="geetest_success_pie"></view>
            <view class="geetest_success_filter"></view>
            <view class="geetest_success_mask"></view>
          </view>
          <view class="geetest_success_correct">
            <view class="geetest_success_icon"></view>
          </view>
        </view>
        <view class="geetest_success_radar_tip">
          <span class="geetest_success_radar_tip_content">{{lang_tips.fullpage.success}}</span>
        </view>
        <label class="geetest_success_logo" target="_blank"></label>
        <view class="geetest_success_offline geetest_offline"></view>
      </view>
    </view>
    <view class="geetest_slide_icon"></view>
  </view>
  <view class="geetest_panel">
    <view class="geetest_panel_ghost"></view>
    <view class="geetest_panel_box">
      <view class="geetest_other_offline geetest_panel_offline" ></view>
      <view class="geetest_panel_loading">
        <view class="geetest_panel_loading_icon"></view>
        <view class="geetest_panel_loading_content"></view>
      </view>
      <view class="geetest_panel_success {{success_animate}}">
        <view class="geetest_panel_success_box">
          <view class="geetest_panel_success_show">
            <view class="geetest_panel_success_pie"></view>
            <view class="geetest_panel_success_filter"></view>
            <view class="geetest_panel_success_mask"></view>
          </view>
          <view class="geetest_panel_success_correct">
          <view class="geetest_panel_success_icon"></view>
        </view>
      </view>
      <view class="geetest_panel_success_title"></view>
    </view>
    <view class="geetest_panel_error">
      <view class="geetest_panel_error_icon"></view>
      <view class="geetest_panel_error_title"></view>
      <view class="geetest_panel_error_content"></view>
      <view class="geetest_panel_error_code"></view>
    </view>
    <view class="geetest_panel_footer">
      <view class="geetest_panel_footer_logo"></view>
      <view class="geetest_panel_footer_copyright"></view>
    </view>
    <view class="geetest_panel_next"></view>
    </view>
  </view>
</view>
<view wx:if="{{loading && iShow}}" class="geetest_holder geetest_wind"><button>{{lang_tips.fullpage.loading}}</button></view>
<view>
</view>
<view wx:if="{{showclick}}" class="geetest_holder geetest_silver geetest_popup" style='display:block;opacity:1'>
  <view class="geetest_popup_ghost" bindtap='hideClickPopup'></view>
  <view class="geetest_popup_box" bindtap='geetest_tap' bindlongtap='geetest_longtap' bindtouchstart='geetest_touchstart' bindtouchmove='geetest_touchmove' bindtouchend='geetest_touchend' bindtouchcancel='geetest_touchcancel'>
    <view class="geetest_widget">
      <view class="geetest_head">
        <view class="geetest_tips">
          <view class="geetest_tip_content" style='margin-top:{{header.space}}' wx:if='{{showclickHeader}}'>
          {{lang_tips.click.conTip1}}<span class="geetest_mark" style='color:{{styleConfig.color}}'>{{lang_tips.click.conTip2}}</span>{{lang_tips.click.conTip3}}</view>
          <view wx:if='{{imgc.src && !imgc.hide}}' class="geetest_tip_img" style="width: 116px; height: 40px; right: -116px; top: -10px; display: block; background-image: url({{imgc.src}});"></view>
        </view>
        <view class="geetest_atips">
          <view class="geetest_atip_content" wx:if='{{!showclickHeader}}'>
           {{lang_tips.click.conTip1}}<span class="geetest_mark" style='color:{{styleConfig.color}}'>{{lang_tips.click.conTip2}}</span>{{lang_tips.click.conTip3}}
          </view>
          <view class='geetest_tip_text {{phrase_num}}'>
              <view class='{{item.className}} {{item.isShow}} geetest_view' wx:for = "{{phrase}}" wx:key="index">
                  <text class='geetest_text'>{{index+1}}</text>
              </view>
          </view>
        </view>
      </view>
      <view class="geetest_table_box">
        <view class="geetest_window">
          <view wx:if='{{isClick || !imgc.src }}' class="geetest_item geetest_big_item" style="width: 95.8%; padding-bottom: 95.8%;" id="img-infos">
            <view class="geetest_item_loading">
              <view class="geetest_item_loading_icon"></view>
              <view class="geetest_item_loading_tip">{{lang_tips.click.loading}}</view>
            </view>
            <view class="geetest_item_wrap"  bindtap='addMarks'>
              <image class="geetest_item_img" src="{{imgc.src}}" style="right: 0px; top: 0px; width: 100%; height: 112%;"></image>
            </view>
            <view class="geetest_big_mark geetest_mark_show" wx:for = "{{marks}}" wx:key="index" style="{{item.style}};background-color:{{styleConfig.color}};" bindtap="remove" data-index="{{index}}">
                <view class="geetest_mark_no">{{index+1}}</view>
            </view>
          </view>
          <view class='geetest_item' style='width:31%;padding-bottom:31%' wx:for = "{{nines}}" wx:key="index">
              <view class='geetest_item_wrap'>
                    <image class='geetest_item_img' src='{{item.src}}' style='{{item.style}}'></image>
              </view>
              <view class='geetest_item_ghost {{item.classname}}' data-answes='{{item.ans}}' data-classsindex='{{index}}' bindtap='addNines'>
                  <view class='geetest_item_icon'></view>
              </view>
          </view>
        </view>
        <view class="geetest_result_tip {{click.tips_class}}">{{click.tips_text}}</view>
      </view>
      <view class="geetest_panel">
        <view class="geetest_small">
          <view class='geetest_close' bindtap='hideClickPopup'>
          </view>
          <view class='geetest_refresh' bindtap='refreshClick'></view>
        </view>
        <label class='geetest_commit' catchtap='validateBtn'><view class="geetest_commit_tip" >{{lang_tips.click.comfirm}}</view></label>
      </view>
    </view>
  </view>
</view>
<view wx:if="{{showslide}}" class="geetest_holder geetest_mobile geetest_ant geetest_popup">
  <view class="geetest_popup_ghost" bindtap='hidePopup'></view>
  <view class="geetest_popup_box">
    <view class="geetest_popup_wrap">
      <view class="geetest_wrap">
        <view class="geetest_widget">
          <view class="geetest_window " style="padding-bottom: 61.54%;">
            <label class="geetest_link">
              <view class="geetest_div_img geetest_absolute">
                <view class="geetest_slicebg geetest_absolute">
                  <view class="geetest_div_bg geetest_absolute" style='height:{{slideConfig.slideHeight}};font-size:0px'>
                    <block wx:for="{{slideConfig.bgPosition}}" wx:key="index">
                      <view wx:if="{{slideConfig.bg}}" class='geetest_view' style='background-position:{{item}};background-image:url("{{slideConfig.bg}}")'></view>
                    </block>
                  </view>
                  <view wx:if="{{slideConfig.slice}}" class="geetest_div_slice geetest_absolute" style='top:{{slideConfig.sliceTop}};width:{{slideConfig.sliceWidth}};height:{{slideConfig.sliceHeight}};transform: translate({{translateX}}px, 0px);background-repeat:no-repeat;")'>
                  <image src="{{slideConfig.slice}}" style="width:{{slideConfig.sliceWidth}}px;height:{{slideConfig.sliceHeight}}px" ></image></view>
                </view>
              </view>
            </label>
            <view class="geetest_refresh" href="javascript:;" style="display: block;">
              <view class="geetest_refresh_tip"></view>
            </view>
            <view class="geetest_loading geetest_absolute geetest_fade" style="padding-top: 10%; opacity: 1; display: block;z-index:-1">
              <view class="geetest_loading_icon"></view>
              <view class="geetest_loading_tip">{{lang_tips.slide.loading}}</view>
            </view>
            <view class="geetest_result {{vrts.result_status}}">
              <view class="geetest_result_box" style="padding-top: 10%;">
                <view class="geetest_result_icon {{vrts.icon_status}}"></view>
                <view class="geetest_result_title">{{vrts.result_text1}}</view>
                <view class="geetest_result_content">{{vrts.result_text2}}</view>
              </view>
            </view>
          </view>
        </view>
        <view class="geetest_slider geetest_ready">
          <view class="geetest_slider_track">
            <view class="geetest_slider_tip geetest_fade">{{lang_tips.slide.slide}}</view>
          </view>
          <view class="geetest_slider_button" style="transform: translate({{translateX}}px, 0px);background-clolr:red" bindtouchstart="slideButtonTouchStart" catchtouchmove="slideButtonTouchMove" catchtouchend="slideButtonTouchEnd" id='geetest_slider_button'>
            <view class='geetest_slider_icon' style='background-color:{{styleConfig.color}};border-left:2px solid {{styleConfig.color}};border-right:2px solid {{styleConfig.color}};' hidden="{{show_icon}}"></view>

            <view class='geetest_slider_move' hidden="{{!show_icon}}">
              <view class='geetest_left_triangle' style='border-right: 7px solid {{styleConfig.color}}'></view>
              <view class='geetest_center_scanle' style='background-color: {{styleConfig.color}}'></view>
              <view class='geetest_right_triangle' style='border-left: 7px solid {{styleConfig.color}}'></view>
            </view>

          </view>
        </view>
      </view>
      <view class="geetest_panel">
        <view class="geetest_small">
          <view class="geetest_close" bindtap='hidePopup'></view>
          <view class='geetest_refresh_1' bindtap='refresh'></view>
        </view>
        <label class="geetest_copyright" style="display:{{logodp}}">
          <view class="geetest_logo"></view>
          <view class="geetest_copyright_tip">{{lang_tips.slide.logo}}</view>
        </label>
      </view>
    </view>
  </view>
</view>

<image wx:for="{{ imgLoadList }}" wx:key="*this" src="{{ item }}" data-src="{{ item }}" bindload="imgLoad" binderror="imgFail" style="width:0;height:0;opacity:0;display:none" />